
<h1>Editing Book</h1>

<%= form_with model: @list, url:update_list_path(@list.id), method: :patch do |f| %>

<h4>Tilte</h4>
<%= f.text_field :title %>

<h4>Body</h4>
<%= f.text_area :body %>

<%= f.submit 'Update Book' %>

<p>
<span><%= link_to "back", '/lists' %></span>
</p>
<p>
<span><%= link_to "show", list_path(@list.id) %></span>
</p>





<% end %>